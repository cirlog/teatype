# Third-party imports
from teatype.cli import BaseIsRunningCLI

class IsRunning(BaseIsRunningCLI):
    #########
    # Hooks #
    #########
    
    def pre_execute(self):
        self.process_names = ['student_db_server', 'pnpm', 'vite']

if __name__ == '__main__':
    IsRunning()